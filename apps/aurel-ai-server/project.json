{
  "name": "aurel-ai-server",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/aurel-ai-server/src",
  "projectType": "application",
  "type": "module",
  "targets": {
    "serve": {
      "executor": "@nx/js:node",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "aurel-ai-server:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "aurel-ai-server:build:development"
        },
        "production": {
          "buildTarget": "aurel-ai-server:build:production"
        }
      }
    },
    "docker-build": {
      "dependsOn": [
        "build"
      ],
      "command": "docker build -f apps/aurel-ai-server/Dockerfile . -t aurel-ai-server"
    }
  },
  "tags": []
}